{
  "name": "Globe-Trotter",
  "version": "1.0.0",
  "engines": {
    "node": "8.11.4",
    "npm": "5.3.0"
  },
  "description": "A web application",
  "main": "./server/app.js",
  "scripts": {
    "watchAppBundle": "babel --watch ./client/app ./client/helper --presets react --out-file ./hosted/bundle.js",
    "watchSettingsBundle": "babel --watch ./client/settings ./client/helper --presets react --out-file ./hosted/settingsBundle.js",
    "watchPinsBundle": "babel --watch ./client/pins ./client/helper --presets react --out-file ./hosted/pinsBundle.js",
    "buildSettings": "babel ./client/settings/ ./client/helper --presets react --out-file ./hosted/settingsBundle.js",
    "build": "gulp build",
    "watch": "gulp watch",
    "start": "node ./server/app.js",
    "pretest": "eslint ./server --fix",
    "test": "echo \"Tests complete\""
  },
  "keywords": [
    "Node",
    "MVC",
    "Redis",
    "Login"
  ],
  "author": "Cody Van De Mark",
  "license": "Apache 2.0",
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "babel": "^6.23.0",
    "body-parser": "^1.15.0",
    "clientside-require": "^4.7.2",
    "compression": "^1.6.1",
    "connect-redis": "^3.0.2",
    "cookie-parser": "^1.3.3",
    "csurf": "^1.8.3",
    "event-stream": "^4.0.1",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "google-maps": "^3.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^5.0.0",
    "gulp-nodemon": "^2.4.1",
    "gulp-sass": "^4.0.2",
    "helmet": "^1.3.0",
    "mongoose": "^4.4.6",
    "serve-favicon": "^2.3.0",
    "svg-world-map": "^1.0.0",
    "underscore": "^1.8.3",
    "world-map": "0.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.1",
    "nodemon": "^1.11.0"
  }
